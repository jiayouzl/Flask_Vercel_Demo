{
    "version": 2,
    "builds": [
        {
            "env": {
                "PYTHON_VERSION": "3.9"
            }
        },
        {
            "src": "/api/app.py",
            "use": "@vercel/python"
        },
        { "src": "/api/static/**", "use": "@vercel/static" }
    ],
    "routes": [
        { "src": "/static/(.*)", "dest": "/api/static/$1" },
        {
            "src": "/(.*)",
            "dest": "/api/app.py"
        }
    ],
    "env": {
        "PYTHONPATH": "api",
        "ECHOTEST": "ABCD0571"
    }
}